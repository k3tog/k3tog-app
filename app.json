{
  "expo": {
    "name": "k3tog-app",
    "slug": "k3tog-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/k3tog_logo_square@4x-icon.png",
    "scheme": "k3tog",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/k3tog_logo_square@4x-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f8f4fb"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/k3tog_logo_square@4x-icon.png",
        "backgroundColor": "#f8f4fb"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/k3tog_logo_square@4x-icon.png"
    },
    "plugins": ["expo-router", "expo-font"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
